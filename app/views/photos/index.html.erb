<%= form_for Photo.new, html: {mulitpart: true} do |f| %>
  <%= f.file_field :image, multiple: true, name: "photo[image]", id: "file_field" %>
<% end %>

<div id = "photos">
  <% @photos.each do |photo| %>
  <div class="photo">
    <!-- #photo.file_name -->
    <%= image_tag photo.image_url.to_s, id: "photo_image" %>
  </div>
  <% end %>
</div>

<script id="template-upload" type="text/x-tmpl">
  <div class="upload">
    {%=o.name%}
    <div class="progress"><div class="bar" style="width: 0%;"></div></div>
  </div>
</script>
